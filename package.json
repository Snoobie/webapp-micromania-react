{
  "name": "este",
  "private": true,
  "engines": {
    "node": ">= 8.0.0"
  },
  "scripts": {
    "dev": "next",
    "build": "rimraf node_modules/.cache/babel-loader && next build",
    "start": "next start",
    "flow": "flow",
    "prettier": "prettier --bracket-spacing --single-quote --tab-width 2 --trailing-comma all --parser babylon --print-width 80  --write \"{{components,lib,pages,scripts,themes,types},__{tests,mocks}__}/**/*.js\"",
    "jest": "jest",
    "jest:watch": "jest --watch",
    "test": "flow && jest && next build",
    "deploy": "node ./scripts/deploy",
    "favicon": "node ./scripts/favicon",
    "relay": "relay-compiler --src ./pages/ --schema data/schema.graphql",
    "get-schema": "node scripts/get-schema.js",
    "upgrade": "rimraf node_modules yarn.lock flow-typed && yarn && flow-typed install"
  },
  "dependencies": {
    "apollo-client": "^1.0.0",
    "auth0-lock": "^10.14.0",
    "color": "^1.0.3",
    "fela": "^4.3.2",
    "fela-plugin-placeholder-prefixer": "^4.3.2",
    "fela-preset-web": "^4.3.2",
    "graphql": "^0.10.1",
    "graphql-tag": "^2.2.0",
    "isomorphic-fetch": "^2.2.1",
    "localforage": "^1.5.0",
    "next": "^3.0.0-beta16",
    "nprogress": "^0.2.0",
    "prop-types": "^15.5.9",
    "ramda": "^0.24.1",
    "react": "^15.6.0",
    "react-apollo": "^1.0.0",
    "react-dom": "^15.6.0",
    "react-fela": "^4.3.2",
    "react-redux": "^5.0.3",
    "redux": "^3.6.0",
    "redux-logger": "^3.0.1",
    "redux-observable": "^0.14.1",
    "redux-persist": "^4.8.0",
    "rimraf": "^2.6.1",
    "rxjs": "^5.4.0",
    "smoothscroll-polyfill": "^0.3.5",
    "uuid": "^3.0.1",
    "validator": "^7.0.0"
  },
  "devDependencies": {
    "babel-eslint": "^7.2.1",
    "babel-jest": "^20.0.1",
    "babel-plugin-transform-define": "^1.2.0",
    "babel-preset-es2015": "^6.24.1",
    "eslint": "^3.18.0",
    "eslint-config-airbnb": "^15.0.1",
    "eslint-loader": "^1.7.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^5.0.3",
    "eslint-plugin-react": "^7.0.1",
    "flow-bin": "^0.48.0",
    "jest": "^20.0.1",
    "prettier": "^1.1.0",
    "react-test-renderer": "^15.4.2"
  }
}
