{
  "presets": [
    "next/babel"
  ],
  "env": {
    "development": {
      "plugins": [
        ["transform-define", "./env-config.js"],
        "react-intl"
      ]
    },
    "production": {
      "plugins": [
        ["transform-define", "./env-config.js"],
        ["react-intl", { "messagesDir": "lang/.messages/" }]
      ]
    },
    "test": {
      "presets": [["env", { "modules": "commonjs" }]],
      "plugins": [
        ["transform-define", "./env-config.js"],
        "react-intl"
      ]
    }
  }
}
