{
  "env": {
    "development": {
      "presets": ["next/babel"],
      "plugins": [
        ["transform-define", "./env-config.js"],
        "react-intl"
      ]
    },
    "production": {
      "presets": ["next/babel"],
      "plugins": [
        ["transform-define", "./env-config.js"],
        ["react-intl", { "messagesDir": "lang/.messages/" }]
      ]
    },
    "test": {
      "presets": [["env", { "modules": "commonjs" }], "next/babel"],
      "plugins": [
        ["transform-define", "./env-config.js"],
        "react-intl"
      ]
    }
  }
}
